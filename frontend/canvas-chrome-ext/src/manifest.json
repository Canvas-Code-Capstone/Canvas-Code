{
  "manifest_version": 2,
  "name": "Extension Prototype",
  "description": "Prototype for Canvas Extension",
  "version": "0.1.0",

  "icons": {
  },
  "web_accessible_resources": ["scripts/inject_script.js"],
  "browser_action": {
    "default_popup": "components/popup.html",
    "default_icon": "images/su_emblem.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs"
  ],

  "background": [{
    "scripts": ["frontend/canvas-chrome-ext/src/scripts/background_script.js"]
  }],

  "content_scripts": [{
    "matches": ["https://seattleu.instructure.com/", "*://www.google.com/", "https://canvas.instructure.com/*"],
    "js": ["scripts/Content.js"]
  }]
}